{
  "git.ignoreLimitWarning": true,
  "cSpell.words": ["babez", "Holygrail", "spacebabez"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.cache": true,
    "**/node_modules": true,
    "**/*.lock": true
  },
  "python.formatting.provider": "autopep8"
}
